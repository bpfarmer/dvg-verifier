{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "<all_urls>"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["pdf.js", "nacl.min.js", "jquery.js", "sha256.js", "app.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": ["pdf.worker.js", "key-ext-style-29.css"]
}
